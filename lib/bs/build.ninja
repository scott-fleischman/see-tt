bs_package_flags = -bs-package-map reason-sandbox
src_root_dir = /Users/joshwestbury/Documents/Learning/reason-sandbox
bsc = /usr/local/lib/node_modules/bs-platform/lib/bsc.exe
bsdep = /usr/local/lib/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/Users/joshwestbury/Documents/Learning/reason-sandbox/node_modules/bs-platform/lib/ocaml' -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = -ns ReasonSandbox
bsb_dir_group = 0
refmt = /usr/local/lib/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I . -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/Demo.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/Demo.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/Demo.mlast.d : build_deps src/Demo.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/Demo-ReasonSandbox.cmj |  $src_root_dir/src/Demo.bs.js src/Demo-ReasonSandbox.cmi : build_cmj_cmi src/Demo.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/Person.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/Person.re
build src/Person.mlast.d : build_deps src/Person.mlast
build src/Person-ReasonSandbox.cmj |  $src_root_dir/src/Person.bs.js src/Person-ReasonSandbox.cmi : build_cmj_cmi src/Person.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/SecondPerson.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/SecondPerson.re
build src/SecondPerson.mlast.d : build_deps src/SecondPerson.mlast
build src/SecondPerson-ReasonSandbox.cmj |  $src_root_dir/src/SecondPerson.bs.js src/SecondPerson-ReasonSandbox.cmi : build_cmj_cmi src/SecondPerson.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build ReasonSandbox.cmi : build_package ReasonSandbox.mlmap
build build.ninja : phony  ||  ReasonSandbox.cmi src/SecondPerson.mlast.d src/Person.mlast.d src/Demo.mlast.d
